{
    "name": "orkhanahmadov/goldenpay",
    "description": "GoldenPay payment library for PHP and Laravel framework",
    "keywords": [
        "orkhanahmadov",
        "azerbaijan",
        "goldenpay",
        "payment"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "blastcloud/guzzler": "^1.4",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\Goldenpay\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\Goldenpay\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
