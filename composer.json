{
    "name": "orkhanahmadov/goldenpay",
    "description": "GoldenPay payment library for PHP and Laravel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Orkhan I. Ahmadov",
            "email": "ahmadov90@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "blastcloud/guzzler": "^1.4",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
        "phpunit/phpunit": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\Goldenpay\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\Goldenpay\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orkhanahmadov\\Goldenpay\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Goldenpay": "Orkhanahmadov\\Goldenpay\\Laravel\\Facades\\Goldenpay"
            }
        }
    }
}
